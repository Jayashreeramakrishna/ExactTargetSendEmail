        </bindings>
        <client>
            <endpoint address="https://webservice.exacttarget.com/Service.asmx"
                binding="basicHttpBinding" bindingConfiguration="SoapBinding"
                contract="ExactTargetService.Soap" name="Soap" />
        </client>
    </system.serviceModel>
